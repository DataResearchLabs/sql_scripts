-- --contact
select * from qw_prod.contact
WHERE QID IN
(SELECT memberQID FROM qw_prod.householdMembers a
where householdQID IN
(Select QID from qw_prod.household WHERE QID like '%FIWM%' AND (name LIKE '%FAKE%' OR name LIKE'%TEST%') AND deletedFlag=0 AND currentFlag =1));

-- Update contact
update qw_prod.contact
set deletedFlag ='1', currentFlag='0', lastUpdated= now()
WHERE QID IN
(SELECT memberQID FROM qw_prod.householdMembers a
where householdQID IN
(Select QID from qw_prod.household WHERE QID like '%FIWM%' AND (name LIKE '%FAKE%' OR name LIKE'%TEST%') AND deletedFlag=0 AND currentFlag =1));

-- household 
SELECT * FROM qw_prod.household
WHERE QID IN
(SELECT householdQID FROM qw_prod.householdMembers a
where householdQID IN
(Select QID from qw_prod.household WHERE QID like '%FIWM%' AND (name LIKE '%FAKE%' OR name LIKE'%TEST%') AND deletedFlag=0 AND currentFlag =1));
 
 -- update household
 update qw_prod.household
 set deletedFlag ='1', currentFlag='0', lastUpdated= now()
 WHERE QID IN
(SELECT householdQID FROM qw_prod.householdMembers 
where householdQID IN
(Select QID from qw_prod.household WHERE QID like '%FIWM%' AND (name LIKE '%FAKE%' OR name LIKE'%TEST%') AND deletedFlag=0 AND currentFlag =1));

-- householdMembers
 SELECT memberQID,m.* FROM qw_prod.householdMembers m
 where householdQID IN
 (Select QID from qw_prod.household WHERE QID like '%FIWM%' AND (name LIKE '%FAKE%' OR name LIKE'%TEST%') AND deletedFlag=0 AND currentFlag =1);
 
 update householdmembers
 set deletedFlag ='1', currentFlag='0', lastUpdated= now()
 where householdQID IN
 (Select QID from qw_prod.household WHERE QID like '%FIWM%' AND (name LIKE '%FAKE%' OR name LIKE'%TEST%') where (firstName like '%TEST%' or firstName like '%FAKE%'));
 
 -- users
  select * from users where (firstName like '%TEST%' or firstName like '%FAKE%' AND deletedFlag=0 AND currentFlag =1); 
  
  update users
  set deletedFlag ='1', currentFlag='0', lastUpdated= now()
  where (firstName like '%TEST%' or firstName like '%FAKE%' AND deletedFlag=0 AND currentFlag =1);
 

  
  
  
